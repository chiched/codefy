{

    "manifest_version": 2,
    "name": "Codefy",
    "version": "1.0",
  
    "description": "CHanges the page to you like coe",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "run_at": "document_start",
        "js": ["start.js"],
        "css": ["beforeLoad.css"]
      },
      {
        "matches": ["<all_urls>"],
        "run_at": "document_end",
        "js": ["end.js"]
      }
      
    ],

    "web_accessible_resources": [
      "style.css",
      "icons/favicon.ico"
    ]
  
  }

